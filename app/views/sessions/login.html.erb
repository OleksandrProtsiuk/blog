<h1>Login</h1>
<p></p>
<div class="container">
  <%= form_tag(log_in_path, method: :post) do %>
    <p>
      Name<br>
      <%= text_field_tag(:name) %>
    </p>
    <p>
      Password<br>
      <%= password_field_tag(:password) %>
    </p>
    <p>
      <%= submit_tag 'LogIn', { :class => 'btn btn-success'} %>
    </p>
    <% end %>
</div>
